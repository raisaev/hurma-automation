{
    "name": "r.isaev/hurma-automation",
    "type": "project",
    "require": {
        "php": "^8.1",
        "symfony/console": "^5.3",
        "symfony/dotenv": "5.3.*",
        "symfony/runtime": "5.3.*",
        "symfony/yaml": "5.3.*",
        "google/apiclient": "^2.0",
        "symfony/dependency-injection": "^5.3",
        "symfony/config": "^5.3",
        "php-webdriver/webdriver": "^1.0",
        "monolog/monolog": "^2.3",
        "symfony/filesystem": "^5.3",
        "symfony/http-kernel": "^5.3",
        "symfony/routing": "^5.3",
        "symfony/templating": "^6.0"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.7",
        "vimeo/psalm": "^4.18"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "bin": [
        "bin/console"
    ],
    "config": {
        "allow-plugins": {
            "symfony/runtime": true
        }
    }
}
